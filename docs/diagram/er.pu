@startuml er

entity "セルフケア分類" as self_care_classification {
  id
  --
  name [varchar]
  order_number[smallint]
  kind[tinyint]
}

entity "セルフケア" as self_care {
  id
  self_care_classification_id[FK]
  --
  log_date[date]
  reason[text]
  point[tinyint|1~10]
  am_pm[tinyint|0~1]
}

self_care_classification ----{ self_care

@enduml
